{
    "type": "package",
    "name": "jfmengels/elm-review-simplification",
    "summary": "Provides elm-review rules to simplify Elm code",
    "license": "BSD-3-Clause",
    "version": "1.0.0",
    "exposed-modules": [
        "NoBooleanCaseOf",
        "NoFullyAppliedPrefixOperator",
        "NoListLiteralsConcat"
    ],
    "elm-version": "0.19.1 <= v < 0.20.0",
    "dependencies": {
        "elm/core": "1.0.5 <= v < 2.0.0",
        "jfmengels/elm-review": "2.3.11 <= v < 3.0.0",
        "stil4m/elm-syntax": "7.2.1 <= v < 8.0.0"
    },
    "test-dependencies": {
        "elm-explorations/test": "1.2.2 <= v < 2.0.0"
    }
}
